        <Button HorizontalAlignment="Left" Margin="10,236,0,0" VerticalAlignment="Top" Width="353" Height="38" Click="Button_Click" Content="Добавить аудиторию"/>


        <ScrollViewer Name="scr" HorizontalAlignment="Left" Height="139" Margin="10,92,0,0" VerticalAlignment="Top" Width="353"/>
        <CheckBox x:Name="chbClose" Content="Выход" HorizontalAlignment="Left" Margin="285,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Тип аудитории" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" RenderTransformOrigin="0,0.538"/>
